{
  "name": "3d-cad-ai",
  "version": "1.0.0",
  "description": "Advanced 3D CAD application with AI integration, PCB design, and simulation capabilities",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode development --config webpack.config.js",
    "build:dev": "webpack --mode development --config webpack.config.js",
    "build:prod": "webpack --mode production --config webpack.config.js",
    "build": "npm run build:prod",
    "test": "jest",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.(js|ts|tsx)$",
    "test:integration": "jest --testPathPattern=tests/integration/.*\\.test\\.(js|ts)$",
    "test:e2e": "playwright test",
    "test:performance": "node scripts/performance-test.js",
    "test:a11y": "axe-core tests/accessibility/",
    "test:cross-browser": "playwright test --config=playwright.cross-browser.config.js",
    "test:smoke": "node scripts/smoke-test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx --format json --output-file eslint-report.json",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --format json --output-file prettier-report.json",
    "type-check": "tsc --noEmit",
    "analyze:bundle": "webpack-bundle-analyzer dist/main.js --mode static --report bundle-analyzer-report.html",
    "package:linux": "electron-builder --linux",
    "package:win32": "electron-builder --win",
    "package:darwin": "electron-builder --mac",
    "docs:generate": "typedoc src/ --out docs/api/",
    "docs:deploy": "gh-pages -d docs/",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "clean": "rimraf dist/ coverage/ node_modules/.cache/",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "keywords": [
    "3d",
    "cad",
    "ai",
    "pcb",
    "simulation",
    "design",
    "engineering",
    "manufacturing"
  ],
  "author": "3D-CAD-AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LexiconAngelus93/3D-CAD-AI.git"
  },
  "bugs": {
    "url": "https://github.com/LexiconAngelus93/3D-CAD-AI/issues"
  },
  "homepage": "https://github.com/LexiconAngelus93/3D-CAD-AI#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.158.0",
    "@types/three": "^0.158.0",
    "typescript": "^5.3.0",
    "@tensorflow/tfjs": "^4.15.0",
    "mathjs": "^12.2.0",
    "plotly.js": "^2.27.0",
    "express": "^4.18.2",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-bundle-analyzer": "^4.10.0",
    "html-webpack-plugin": "^5.6.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.5.0",
    "path-browserify": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "stream-browserify": "^3.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@playwright/test": "^1.40.0",
    "axe-core": "^4.8.0",
    "@axe-core/playwright": "^4.8.0",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.12.0",
    "size-limit": "^11.0.0",
    "@size-limit/preset-app": "^11.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "typedoc": "^0.25.0",
    "gh-pages": "^6.1.0",
    "electron": "^27.1.0",
    "electron-builder": "^24.8.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/main.js",
      "limit": "5 MB"
    }
  ],
  "build": {
    "appId": "com.3dcadai.app",
    "productName": "3D CAD AI",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}

